{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow, noarchive, nosnippet"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/page/:page",
      "destination": "/index.html"
    },
    {
      "source": "/tanusitvanyok",
      "destination": "/index.html"
    },
    {
      "source": "/segitseg",
      "destination": "/index.html"
    },
    {
      "source": "/rss",
      "destination": "/index.html"
    },
    {
      "source": "/impresszum",
      "destination": "/index.html"
    },
    {
      "source": "/kapcsolat",
      "destination": "/index.html"
    },
    {
      "source": "/jogszabalyi-hatter",
      "destination": "/index.html"
    },
    {
      "source": "/akadalymentesitesi-nyilatkozat",
      "destination": "/index.html"
    }
  ]
}
